{
  "name": "chatbot-shain-v2",
  "version": "1.0.0",
  "main": "server.js",
  "type": "module",
  "imports": {
    "#*": "./src/*",
    "#test/*": "./test/*"
  },
  "scripts": {
    "start": "node server.js",
    "dev": "node --watch --env-file=.env server.js",
    "test": "node --test",
    "test:watch": "node --test --watch",
    "test:coverage": "c8 --reporter=html --reporter=text --reporter=lcov npm test",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "prepare": "node -e \"try { require('husky').install() } catch (e) {}\""
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "Configuracion moderna para proyectos con NodeJS",
  "dependencies": {
    "axios": "^1.13.2",
    "cookie-parser": "^1.4.7",
    "express": "^5.2.1",
    "express-rate-limit": "^8.2.1",
    "helmet": "^8.1.0",
    "openai": "^6.10.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.15.0",
    "c8": "^10.1.2",
    "eslint": "^9.15.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "globals": "^15.12.0",
    "husky": "^9.1.6",
    "lint-staged": "^15.2.10",
    "prettier": "^3.3.3"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
